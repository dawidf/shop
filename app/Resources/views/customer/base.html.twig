<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Dumplie{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="//cdn.rawgit.com/twbs/bootstrap/v4-dev/dist/css/bootstrap.css">
        <link rel="stylesheet" href="{{ asset('assets/css/customer/style.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
</head>
<body>
{% block header %}
<header>
    <div class="container">
        <nav class="navbar navbar-dark bg-primary">
            <div class="nav navbar-nav">
                <a class="nav-item nav-link active" href="{{ url('dumplie_customer_homepage') }}">Home</a>
            </div>
        </nav>
    </div>
</header>
{% endblock %}
<div class="container">
    {% block body %}{% endblock %}
</div>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/tether/1.3.3/js/tether.min.js"></script>
<script src="//code.jquery.com/jquery-2.2.4.min.js"></script>
<script src="//cdn.rawgit.com/twbs/bootstrap/v4-dev/dist/js/bootstrap.js"></script>
{% block javascripts %}{% endblock %}
</body>
</html>
